{
  "name": "billbook-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc --noEmit && vite build",
    "build:dev": "vite build --mode development",
    "lint": "eslint . && tsc --noEmit",
    "lint:fix": "eslint . --fix",
